{
  "name": "vue-progressbar",
  "version": "0.8.0",
  "description": "A lightweight progress bar for vue",
  "main": "dist/vue-progressbar.es5.min.js",
  "scripts": {
    "build": "npm run copyfiles",
    "copyfiles": "npm run copy:js && npm run copy:assets && npm run copy:vue",
    "copy:assets": "copyfiles -f ./src/assets/*.js ./src/assets/*.css dist/assets",
    "copy:js": "copyfiles -f ./src/*.js dist",
    "copy:vue": "copyfiles -f ./src/*.vue dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hilongjw/vue-progressbar.git"
  },
  "keywords": [
    "vue",
    "vue-progressbar",
    "vue loading progress",
    "vue progressbar",
    "progressbar"
  ],
  "author": [
    {
      "name": "XeonPowder",
      "email": "lmvdzd@gmail.com"
    }, {
      "name": "Awe",
      "email": "hilongjw@qq.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hilongjw/vue-progressbar/issues"
  },
  "homepage": "https://github.com/hilongjw/vue-progressbar#readme",
  "devDependencies": {
    "copyfiles": "^1.0.0"
  }
}
