{
  "name": "vue-progressbar",
  "version": "0.7.2",
  "description": "A lightweight progress bar for vue",
  "main": "dist/vue-progressbar.es5.min.js",
  "scripts": {
    "build": "npm run copyfiles && npm run build:ugly",
    "build:ugly": "uglifyjs dist/vue-progressbar.es5.js -cm --comments -o dist/vue-progressbar.es5.min.js --source-map dist/vue-progressbar.es5.js.map",
    "copyfiles": "copyfiles -f ./src/*.js ./src/*.vue dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hilongjw/vue-progressbar.git"
  },
  "keywords": [
    "vue",
    "vue-progressbar",
    "vue loading progress",
    "vue progressbar",
    "progressbar"
  ],
  "author": [{
    "name": "XeonPowder",
    "email": "lmvdzd@gmail.com"
  }, {
    "name": "Awe",
    "email": "hilongjw@qq.com"
  }],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hilongjw/vue-progressbar/issues"
  },
  "homepage": "https://github.com/hilongjw/vue-progressbar#readme",
  "dependencies": {
    "copyfiles": "^1.0.0",
    "uglify-js": "^2.7.4"
  }
}
